{
  "verseE": [
    {
      "name": "En effet, l'Eternel connaît la voie des justes, mais la voie des méchants mène à la ruine.",
      "verse": "Psaumes 1 : 6",
      "letter": "E",
      "bgVerses": "https://media.githubusercontent.com/media/maxtanie/divine-words-img/master/img/E/desktop-img/img-psaumes-1-6.png",
